<%- model_class = OperadorVirtual -%>
<div class="page-header">
  <h1><%=t '.title', :default => OperadorVirtual %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:descripcion) %></dt>
      <dd><pre class="prettyprint"><%= @operador_virtual.descripcion %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              operador_virtuals_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_operador_virtual_path(@operador_virtual), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              operador_virtual_path(@operador_virtual),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

