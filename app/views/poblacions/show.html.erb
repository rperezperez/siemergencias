<%- model_class = Poblacion -%>
<div class="page-header">
  <h1><%=t '.title', :default => Poblacion %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:nombre) %></dt>
      <dd><pre class="prettyprint"><%= @poblacion.nombre %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:codigo) %></dt>
      <dd><pre class="prettyprint"><%= @poblacion.codigo %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:pl) %></dt>
      <dd><pre class="prettyprint"><%= @poblacion.pl %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:gc) %></dt>
      <dd><pre class="prettyprint"><%= @poblacion.gc %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:epes) %></dt>
      <dd><pre class="prettyprint"><%= @poblacion.epes %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              poblacions_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_poblacion_path(@poblacion), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              poblacion_path(@poblacion),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

