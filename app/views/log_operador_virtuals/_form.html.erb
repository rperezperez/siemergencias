<%= simple_form_for(@log_operador_virtual) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :operador_virtual %>
    <%= f.input :tipo %>
    <%= f.input :mensaje %>
    <%= f.input :fecha %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
