<h1>Listing ios</h1>

<table>
  <thead>
    <tr>
      <th>Device token</th>
      <th>Dispositivo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ios.each do |io| %>
      <tr>
        <td><%= io.device_token %></td>
        <td><%= io.dispositivo %></td>
        <td><%= link_to 'Show', io %></td>
        <td><%= link_to 'Edit', edit_io_path(io) %></td>
        <td><%= link_to 'Destroy', io, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Io', new_io_path %>
